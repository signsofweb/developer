<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Config:etc/system_file.xsd">
    <system>
        <section id="newsletter" translate="label" type="text" sortOrder="110" showInDefault="1" showInWebsite="1" showInStore="1">
            <label>Newsletter</label>
            <tab>customer</tab>
            <resource>Magento_Newsletter::newsletter</resource>
            <group id="subscription_config" translate="label" type="text" sortOrder="1" showInDefault="1" showInWebsite="1" showInStore="1">
                <label>Mailchimp Groups ID Config</label>
                <field id="general_health" translate="label" type="text" sortOrder="1" showInDefault="1" showInWebsite="1" showInStore="1" canRestore="1">
                    <label>General Health</label>
                </field>
				<field id="weight_management" translate="label" type="text" sortOrder="1" showInDefault="1" showInWebsite="1" showInStore="1" canRestore="1">
                    <label>Weight Management</label>
                </field>
				<field id="superfoods" translate="label" type="text" sortOrder="1" showInDefault="1" showInWebsite="1" showInStore="1" canRestore="1">
                    <label>SuperFoods</label>
                </field>
				<field id="kids_health" translate="label" type="text" sortOrder="1" showInDefault="1" showInWebsite="1" showInStore="1" canRestore="1">
                    <label>Kids Health</label>
                </field>
				<field id="e50_health" translate="label" type="text" sortOrder="1" showInDefault="1" showInWebsite="1" showInStore="1" canRestore="1">
                    <label>50+ Health</label>
                </field>
            </group>
            <group id="program_config" translate="label" type="text" sortOrder="1" showInDefault="1" showInWebsite="1" showInStore="1">
                <label>Mailchimp Groups ID Config For Program</label>
                <field id="intensive" translate="label" type="text" sortOrder="1" showInDefault="1" showInWebsite="1" showInStore="1" canRestore="1">
                    <label>Intensive 2 Week Program</label>
                </field>
                <field id="steady" translate="label" type="text" sortOrder="1" showInDefault="1" showInWebsite="1" showInStore="1" canRestore="1">
                    <label>Steady 10+ Week Program</label>
                </field>
                <field id="maintennace" translate="label" type="text" sortOrder="1" showInDefault="1" showInWebsite="1" showInStore="1" canRestore="1">
                    <label>Maintennace 4 Week Program</label>
                </field>
            </group>
        </section>
    </system>
</config>
